{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["./src/components"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },

  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./out/",
    "recurse": true,
    "verbose": true
  }
}
